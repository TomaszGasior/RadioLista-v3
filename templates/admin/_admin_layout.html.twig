{%- trans_default_domain 'admin' -%}

{% extends 'layout.html.twig' %}

{% block head_closing %}
    <link rel="stylesheet" href="{{ asset('css/admin.css') }}">
    <script type="module" src="{{ asset('js/admin.js') }}"></script>
{% endblock %}

{% block page_title %}{{ 'layout.title'|trans }}{% endblock %}

{% block context_menu %}
    <li>
        <a href="{{ path('admin.radio_tables') }}">
            {{ 'radio_tables.title'|trans }}
        </a>
    </li>
    <li>
        <a href="{{ path('admin.users') }}">
            {{ 'users.title'|trans }}
        </a>
    </li>
{% endblock %}

{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "beberlei/doctrineextensions": "^1.1",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.6",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "knplabs/knp-snappy-bundle": "^1.5",
        "knplabs/knp-time-bundle": "^1.11",
        "sensio/framework-extra-bundle": "^5.2",
        "sensiolabs/security-checker": "^6.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.0",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/security-bundle": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.2.*",
        "tgalopin/html-sanitizer-bundle": "^1.1",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "dama/doctrine-test-bundle": "^6.0",
        "deployer/deployer": "^6.7",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "fzaninotto/faker": "^1.8",
        "symfony/browser-kit": "5.2.*",
        "symfony/css-selector": "5.2.*",
        "symfony/debug-bundle": "5.2.*",
        "symfony/maker-bundle": "^1.6",
        "symfony/phpunit-bridge": "5.2.*",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "5.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop --release=custom --custom-build-id=5af1ea2fd3c9442ab08af09ec2423ce6": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security:check": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
